{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "svg.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/assets/*.json"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "./src/assets/cities500.json": [
        "./src/assets/cities500.extract.json"
      ]
    }
  }
}